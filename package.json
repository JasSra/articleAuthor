{
  "name": "code-chronicle",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run gen:api && next build",
    "start": "next start",
    "lint": "next lint",
    "gen:api": "curl -s http://localhost:5229/swagger/v1/swagger.json -o ./api-consolidated-schema.json && npx openapi-typescript-codegen --input ./api-consolidated-schema.json --output api/consolidated --exportModels true",
    "seed:lookups": "tsx ./scripts/seed-lookups.ts",
    "seed:content": "tsx ./scripts/seed-content.ts",
    "seed:forms": "tsx ./scripts/seed-forms.ts",
    "seed:workflows": "tsx ./scripts/seed-workflows.ts",
    "seed:all": "tsx ./scripts/seed-all.ts"
  },
  "dependencies": {
    "@azure/msal-browser": "^3.7.1",
    "@azure/msal-react": "^2.0.9",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.2.0",
    "@tiptap/extension-image": "^2.1.13",
    "@tiptap/extension-placeholder": "^2.1.13",
    "@tiptap/pm": "^2.1.13",
    "@tiptap/react": "^2.1.13",
    "@tiptap/starter-kit": "^2.1.13",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^10.1.0",
